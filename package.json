{
  "name": "@ng-seed/universal",
  "version": "6.0.0",
  "description": "Seed project for Angular Universal apps featuring Server-Side Rendering (SSR), Webpack, dev/prod modes, DLLs, AoT compilation, HMR, SCSS compilation, lazy loading, config, cache, i18n, SEO, and TSLint/codelyzer",
  "repository": {
    "type": "git",
    "url": "https://github.com/ng-seed/universal.git"
  },
  "author": {
    "name": "Burak Tasci",
    "email": "me@fulls1z3.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ng-seed/universal/issues"
  },
  "homepage": "https://github.com/ng-seed/universal#readme",
  "scripts": {
    "clean": "npm run clean:artifacts & rimraf node_modules/.cache coverage test-report.xml",
    "clean:artifacts": "rimraf .cache .server/server* .temp public",
    "build:spa-dev": "npm run clean:artifacts && gulp build:spa-dev --color",
    "build:spa-stage": "npm run clean:artifacts && gulp build:spa-stage --color",
    "build:spa-prod": "npm run clean:artifacts && gulp build:spa-prod --color",
    "build:universal-dev": "npm run clean:artifacts && gulp build:universal-dev --color",
    "build:universal-stage": "npm run clean:artifacts && gulp build:universal-stage --color",
    "build:universal-prod": "npm run clean:artifacts && gulp build:universal-prod --color",
    "serve": "nodemon ./.server/server.js",
    "serve:spa": "nodemon ./.server/spa-server.js",
    "serve:spa-hmr": "node ./.server/spa-server.hmr.js",
    "serve:watch": "npm run build:spa-dev && concurrently -n \"NODE,JEST\" \"npm run serve:spa-hmr\" \"npm run test:watch\"",
    "ci:before": "greenkeeper-lockfile-update",
    "ci:after": "greenkeeper-lockfile-upload",
    "lint": "tslint -p ./tsconfig.json --force",
    "test": "jest --coverage --colors",
    "test:ci": "jest --ci --coverage --colors",
    "test:watch": "jest --runInBand --watch --colors",
    "e2e": "npm run build:universal-dev && concurrently -k --kill-others-on-fail -s first \"npm run serve\" \"jest -c ./tools/test/e2e-preset.json --runInBand --colors\"",
    "e2e:ci": "concurrently -k --kill-others-on-fail -s first \"npm run serve\" \"jest --ci -c ./tools/test/e2e-preset.json --colors\"",
    "docs": "./node_modules/.bin/compodoc -p tsconfig.json -d docs -n \"ng-seed/universal\" -t --theme stripe",
    "release": "standard-version && npm run docs"
  },
  "dependencies": {
    "express": "~4.16.3",
    "debug": "~3.1.0",
    "compression": "~1.7.2",
    "morgan": "~1.9.0",
    "body-parser": "~1.18.3",
    "@angular/animations": "~6.0.2",
    "@angular/common": "~6.0.2",
    "@angular/core": "~6.0.2",
    "@angular/forms": "~6.0.2",
    "@angular/http": "~6.0.2",
    "@angular/platform-browser": "~6.0.2",
    "@angular/platform-browser-dynamic": "~6.0.2",
    "@angular/platform-server": "~6.0.2",
    "@angular/router": "~6.0.2",
    "@angular/cdk": "~6.0.2",
    "@angular/material": "~6.0.2",
    "@angular/flex-layout": "6.0.0-beta.15",
    "core-js": "~2.5.6",
    "rxjs": "~6.1.0",
    "rxjs-compat": "~6.1.0",
    "zone.js": "~0.8.26",
    "request": "~2.86.0",
    "hammerjs": "~2.0.8",
    "lodash": "~4.17.10",
    "@angularclass/bootloader": "~1.0.1",
    "@ngrx/store": "6.0.0",
    "@ngrx/effects": "6.0.0-beta.2",
    "@nguniversal/common": "~6.0.0",
    "@nguniversal/express-engine": "~6.0.0",
    "@ngx-config/core": "6.0.0-rc.1",
    "@ngx-config/http-loader": "6.0.0-rc.1",
    "@ngx-cache/core": "6.0.0-rc.1",
    "@ngx-cache/platform-browser": "6.0.0-rc.1",
    "@ngx-cache/platform-server": "6.0.0-rc.1",
    "@ngx-cache/fs-storage": "6.0.0-rc.1",
    "@ngx-auth/core": "6.0.0-rc.1",
    "@ngx-translate/core": "~10.0.1",
    "@ngx-translate/http-loader": "~3.0.1",
    "@ngx-meta/core": "6.0.0-rc.1",
    "angulartics2": "~6.1.0",
    "ngx-perfect-scrollbar": "~6.0.0"
  },
  "devDependencies": {
    "nodemon": "~1.17.4",
    "@angular/cli": "~6.0.3",
    "@angular/compiler": "~6.0.2",
    "@angular/compiler-cli": "~6.0.2",
    "@angular/language-service": "~6.0.2",
    "@ngrx/schematics": "6.0.0-beta.2",
    "@types/express": "~4.11.1",
    "@types/jest": "~22.2.3",
    "@types/lodash": "~4.14.108",
    "@types/nightmare": "~2.10.4",
    "@types/node": "~10.1.1",
    "rimraf": "~2.6.2",
    "concurrently": "~3.5.1",
    "json-sub": "1.1.0",
    "gulp": "~4.0.0",
    "gulp-load-plugins": "~1.5.0",
    "lazypipe": "~1.0.1",
    "async": "~2.6.0",
    "gulp-changed": "~3.2.0",
    "gulp-ignore": "~2.0.2",
    "gulp-debug": "~4.0.0",
    "gulp-exec": "~3.0.1",
    "gulp-tslint": "~8.1.3",
    "gulp-replace": "~1.0.0",
    "webpack": "~4.8.3",
    "angular-webpack-config": "6.0.1",
    "webpack-merge": "~4.1.2",
    "webpack-dev-middleware": "~3.1.3",
    "webpack-hot-middleware": "~2.22.2",
    "@ngtools/webpack": "~6.0.3",
    "tslint-loader": "~3.6.0",
    "awesome-typescript-loader": "~5.0.0",
    "angular2-template-loader": "0.6.0",
    "ng-router-loader": "~2.1.0",
    "to-string-loader": "~1.1.5",
    "style-loader": "~0.21.0",
    "css-loader": "~0.28.11",
    "postcss-loader": "~2.1.5",
    "autoprefixer": "~8.5.0",
    "stylefmt-loader": "~1.2.3",
    "sass-loader": "~7.0.1",
    "html-loader": "~0.5.5",
    "raw-loader": "~0.5.1",
    "file-loader": "~1.1.11",
    "assets-webpack-plugin": "~3.5.1",
    "copy-webpack-plugin": "~4.5.1",
    "html-webpack-plugin": "~3.2.0",
    "html-elements-webpack-plugin": "~2.0.0",
    "script-ext-html-webpack-plugin": "~2.0.1",
    "hard-source-webpack-plugin": "~0.6.7",
    "optimize-js-plugin": "~0.0.4",
    "uglifyjs-webpack-plugin": "~1.2.5",
    "extract-text-webpack-plugin": "~4.0.0-beta.0",
    "gulp-sass": "~4.0.1",
    "gulp-sourcemaps": "~2.6.4",
    "gulp-postcss": "~7.0.1",
    "postcss-scss": "~1.0.5",
    "postcss-reporter": "~5.0.0",
    "gulp-sass-lint": "~1.3.4",
    "stylelint": "~9.2.1",
    "stylelint-config-standard": "17.0.0",
    "colorguard": "~1.2.1",
    "stylefmt": "~6.0.0",
    "jest": "~22.4.3",
    "jest-preset-angular": "~5.2.2",
    "jest-junit-reporter": "~1.1.0",
    "jasmine-marbles": "~0.3.1",
    "nightmare": "~3.0.1",
    "@compodoc/compodoc": "~1.1.3",
    "standard-version": "~4.3.0",
    "codelyzer": "~4.3.0",
    "tslint": "~5.10.0",
    "angular-tslint-rules": "~1.4.1",
    "tslib": "~1.9.1",
    "typescript": "~2.7.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "./tools/test/jest.setup.ts",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.spec.json"
      },
      "__TRANSFORM_HTML__": true
    },
    "transform": {
      "^.+\\.(css|less|scss|styl|sss)$": "<rootDir>/tools/test/style-processor.js",
      "^.+\\.(ts|js|html)$": "<rootDir>/node_modules/jest-preset-angular/preprocessor.js"
    },
    "moduleNameMapper": {
      "~(.*)$": "<rootDir>/src/client/$1"
    },
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "cache": false,
    "silent": true,
    "collectCoverageFrom": [
      "src/**/*(*.service|*.component|*.pipe|*.directive).{ts,html}",
      "src/**/models/**.ts"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "json-sub",
      "angular2-template-loader",
      "stylelint-config-standard"
    ]
  }
}
