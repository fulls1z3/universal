{
  "name": "@ng-seed/universal",
  "version": "1.2.0",
  "description": "Seed project for Angular Universal apps featuring Server-Side Rendering (SSR), Webpack, dev/prod modes, DLLs, AoT compilation, HMR, SCSS compilation, lazy loading, config, cache, i18n, SEO, and TSLint/codelyzer",
  "repository": {
    "type": "git",
    "url": "https://github.com/ng-seed/universal.git"
  },
  "author": {
    "name": "Burak Tasci",
    "email": "me@fulls1z3.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ng-seed/universal/issues"
  },
  "homepage": "https://github.com/ng-seed/universal#readme",
  "scripts": {
    "clean": "npm run clean:artifacts & rimraf node_modules/.cache coverage test-report.xml",
    "clean:artifacts": "rimraf .cache .server/server* .temp public",
    "build:spa-dev": "npm run clean:artifacts && gulp build:spa-dev --color",
    "build:spa-stage": "npm run clean:artifacts && gulp build:spa-stage --color",
    "build:spa-prod": "npm run clean:artifacts && gulp build:spa-prod --color",
    "build:universal-dev": "npm run clean:artifacts && gulp build:universal-dev --color",
    "build:universal-stage": "npm run clean:artifacts && gulp build:universal-stage --color",
    "build:universal-prod": "npm run clean:artifacts && gulp build:universal-prod --color",
    "serve": "nodemon ./.server/server.js",
    "serve:spa": "nodemon ./.server/spa-server.js",
    "serve:spa-hmr": "node ./.server/spa-server.hmr.js",
    "serve:watch": "npm run build:spa-dev && concurrently -n \"NODE,JEST\" \"npm run serve:spa-hmr\" \"npm run test:watch\"",
    "ci:before": "greenkeeper-lockfile-update",
    "ci:after": "greenkeeper-lockfile-upload",
    "lint": "tslint -p ./tslint.json --force",
    "test": "jest --coverage --colors",
    "test:ci": "jest --ci --coverage --colors",
    "test:watch": "jest --runInBand --watch --colors",
    "e2e": "npm run build:universal-dev && concurrently -k --kill-others-on-fail -s first \"npm run serve\" \"jest -c ./tools/test/e2e-preset.json --runInBand --colors\"",
    "e2e:ci": "concurrently -k --kill-others-on-fail -s first \"npm run serve\" \"jest --ci -c ./tools/test/e2e-preset.json --colors\"",
    "docs": "./node_modules/.bin/compodoc -p tsconfig.json -d docs -n \"ng-seed/universal\" -t --theme stripe",
    "release": "standard-version && npm run docs"
  },
  "dependencies": {
    "express": "~5.0.0-alpha.6",
    "debug": "~3.0.1",
    "compression": "~1.7.0",
    "morgan": "~1.8.2",
    "body-parser": "~1.18.0",
    "@angular/animations": "~4.4.1",
    "@angular/common": "~4.4.1",
    "@angular/core": "~4.4.1",
    "@angular/forms": "~4.4.1",
    "@angular/http": "~4.4.1",
    "@angular/platform-browser": "~4.4.1",
    "@angular/platform-browser-dynamic": "~4.4.1",
    "@angular/platform-server": "~4.4.1",
    "@angular/router": "~4.4.1",
    "@angular/cdk": "2.0.0-beta.10",
    "@angular/material": "2.0.0-beta.10",
    "@angular/flex-layout": "2.0.0-beta.9",
    "core-js": "~2.5.1",
    "rxjs": "~5.4.3",
    "zone.js": "~0.8.17",
    "hammerjs": "~2.0.8",
    "lodash": "~4.17.4",
    "@angularclass/bootloader": "~1.0.1",
    "@ngrx/store": "~4.0.3",
    "@ngrx/effects": "~4.0.5",
    "@nguniversal/express-engine": "~1.0.0-beta.3",
    "@ngx-universal/state-transfer": "4.0.1",
    "@ngx-config/core": "4.0.1",
    "@ngx-config/http-loader": "4.0.1",
    "@ngx-config/fs-loader": "4.0.1",
    "@ngx-cache/core": "4.0.1",
    "@ngx-cache/platform-browser": "4.0.1",
    "@ngx-cache/platform-server": "4.0.1",
    "@ngx-cache/fs-storage": "4.0.1",
    "@ngx-universal/config-loader": "4.0.1",
    "@ngx-auth/core": "4.1.0",
    "@ngx-translate/core": "~6.0.1",
    "@ngx-translate/http-loader": "~0.0.3",
    "@ngx-universal/translate-loader": "4.0.1",
    "@ngx-meta/core": "4.0.1",
    "@ngx-i18n-router/core": "4.0.1",
    "@ngx-i18n-router/config-loader": "4.0.1",
    "angulartics2": "~2.4.0",
    "ngx-perfect-scrollbar": "~4.6.2"
  },
  "devDependencies": {
    "nodemon": "~1.12.0",
    "@angular/compiler": "~4.4.1",
    "@angular/compiler-cli": "~4.4.1",
    "@types/node": "~8.0.28",
    "@types/express": "~4.0.37",
    "@types/jest": "~21.1.0",
    "@types/lodash": "4.14.55",
    "@types/nightmare": "~1.6.35",
    "rimraf": "~2.6.1",
    "concurrently": "~3.5.0",
    "json-sub": "1.1.0",
    "supports-color": "3.2.3",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-load-plugins": "~1.5.0",
    "lazypipe": "~1.0.1",
    "async": "~2.5.0",
    "gulp-changed": "~3.1.0",
    "gulp-ignore": "~2.0.2",
    "gulp-debug": "~3.1.0",
    "gulp-exec": "~2.1.3",
    "gulp-tslint": "~8.1.2",
    "gulp-replace": "~0.6.1",
    "webpack": "~3.6.0",
    "angular-webpack-config": "1.3.0",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony-v2.8.22",
    "webpack-merge": "~4.1.0",
    "webpack-dev-middleware": "~1.12.0",
    "webpack-hot-middleware": "~2.19.1",
    "@ngtools/webpack": "~1.7.0",
    "tslint-loader": "~3.5.3",
    "awesome-typescript-loader": "~3.2.3",
    "angular2-template-loader": "0.6.0",
    "ng-router-loader": "~2.1.0",
    "json-loader": "~0.5.7",
    "to-string-loader": "~1.1.5",
    "style-loader": "~0.18.2",
    "css-loader": "~0.28.7",
    "postcss-loader": "~2.0.6",
    "autoprefixer": "~7.1.4",
    "stylefmt-loader": "~1.2.3",
    "sass-loader": "~6.0.6",
    "html-loader": "~0.5.1",
    "raw-loader": "~0.5.1",
    "file-loader": "~0.11.2",
    "assets-webpack-plugin": "~3.5.1",
    "copy-webpack-plugin": "~4.0.1",
    "html-webpack-plugin": "~2.30.1",
    "html-elements-webpack-plugin": "~1.0.1",
    "script-ext-html-webpack-plugin": "~1.8.5",
    "webpack-dll-bundles-plugin": "~1.0.0-beta.5",
    "hard-source-webpack-plugin": "~0.4.9",
    "add-asset-html-webpack-plugin": "~2.1.1",
    "optimize-js-plugin": "~0.0.4",
    "extract-text-webpack-plugin": "~3.0.0",
    "gulp-sass": "~3.1.0",
    "gulp-sourcemaps": "~2.6.1",
    "gulp-postcss": "~7.0.0",
    "postcss-scss": "~1.0.2",
    "postcss-reporter": "~5.0.0",
    "gulp-sass-lint": "~1.3.3",
    "stylelint": "~8.1.1",
    "stylelint-config-standard": "~17.0.0",
    "colorguard": "~1.2.0",
    "stylefmt": "~6.0.0",
    "jest": "~21.1.0",
    "jest-preset-angular": "~3.0.1",
    "jest-junit-reporter": "~1.1.0",
    "jasmine-marbles": "~0.1.0",
    "nightmare": "~2.10.0",
    "@compodoc/compodoc": "~1.0.1",
    "standard-version": "~4.2.0",
    "codelyzer": "~3.2.0",
    "tslint": "~5.7.0",
    "angular-tslint-rules": "1.0.4",
    "tslib": "~1.7.1",
    "typescript": "~2.5.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "./tools/test/jest.setup.ts",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.spec.json"
      },
      "__TRANSFORM_HTML__": true
    },
    "transform": {
      "^.+\\.(css|less|scss|styl|sss)$": "<rootDir>/tools/test/style-processor.js",
      "^.+\\.(ts|js|html)$": "<rootDir>/node_modules/jest-preset-angular/preprocessor.js"
    },
    "moduleNameMapper": null,
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "cache": false,
    "silent": true,
    "collectCoverageFrom": [
      "src/**/*(*.service|*.component|*.pipe|*.directive).{ts,html}",
      "src/**/models/**.ts"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "@ngx-universal/express-engine",
      "@ngx-translate/core",
      "@ngx-translate/http-loader",
      "@types/lodash",
      "json-sub",
      "supports-color",
      "angular2-template-loader"
    ]
  }
}
